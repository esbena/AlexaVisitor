{
    "name": "AlexaVisitor",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Visits alexa top 1000 pages when clicked",
    "icons": {"16": "icon.png","48": "icon.png","128": "icon.png"},
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "<all_urls>", "tabs"
    ]
}
